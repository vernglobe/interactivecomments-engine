{
  "name": "vern-eforeverliving-engine",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --fix ./src/**/*.ts",
    "watch": "tsc -w",
    "test": "jest --coverage",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/aws-lambda": "^8.10.108",
    "@types/jest": "^27.5.2",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "aws-cdk": "^2.50.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.50.0",
    "aws-sdk": "^2.1253.0",
    "constructs": "^10.1.158",
    "deepmerge": "^4.2.2",
    "source-map-support": "^0.5.21"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "coverageThreshold": {
      "./src/functions/**": {
        "branches": 70,
        "functions": 70,
        "lines": 70
      },
      "./src/packages/**": {
        "branches": 70,
        "functions": 70,
        "lines": 70
      }
    }
  },
  "license": "MIT"
}
